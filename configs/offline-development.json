{
  "name": "Offline Development",
  "description": "Development without external dependencies (no real database, no real notifications)",
  "environment": "development",
  "services": {
    "ILogger": {
      "module": "../src/lib/services/concrete/LoggerService",
      "className": "LoggerService",
      "description": "Real logger for development"
    },
    "IEventBus": {
      "module": "../src/lib/services/concrete/EventBus",
      "className": "EventBus",
      "description": "Real event bus for development"
    },
    "IDatabaseService": {
      "module": "../src/lib/services/mocks/MockDatabase",
      "className": "MockDatabase",
      "isMock": true,
      "description": "Mock database for offline development"
    },
    "IMonitorService": {
      "module": "../src/lib/services/mocks/MockMonitor",
      "className": "MockMonitor",
      "isMock": true,
      "description": "Mock monitoring for offline development"
    },
    "IAlertingService": {
      "module": "../src/lib/services/concrete/AlertingService",
      "className": "AlertingService",
      "description": "Real alerting logic for development"
    },
    "INotificationService": {
      "module": "../src/lib/services/mocks/MockNotification",
      "className": "MockNotification",
      "isMock": true,
      "description": "Mock notifications for offline development"
    },
    "IAuthService": {
      "module": "../src/lib/services/concrete/MockAuth",
      "className": "MockAuth",
      "isMock": true,
      "description": "Mock authentication for development"
    }
  }
}
