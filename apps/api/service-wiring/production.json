      "module": "@network-monitor/database",
      "className": "TargetRepository",
      "isMock": false,
      "description": "Legacy target repository"
    },
    "ISpeedTestRepository": {
      "module": "@network-monitor/database",
      "className": "SpeedTestRepository",
      "isMock": false,
      "description": "Legacy speed test repository"
    },
