      "description": "Alerting service implementation"
    },
    "INotificationService": {
      "module": "@network-monitor/notification",
      "className": "NotificationService",
      "isMock": false,
      "description": "Notification service implementation"
    },
    "IAuthService": {
      "module": "@network-monitor/auth",
      "className": "MockAuth",
      "isMock": true,
      "description": "Mock authentication service (replace with real auth)"
    }
  }
}

