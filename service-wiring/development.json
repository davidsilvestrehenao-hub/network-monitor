{
  "name": "Development Service Wiring",
  "description": "Dependency injection configuration - defines which service implementations to use",
  "WARNING": "⚠️  This is SERVICE WIRING, NOT runtime configuration!",
  "runtimeConfig": "All runtime configuration (database URLs, ports, keys) must be in environment variables (.env file)",
  "purpose": "This file defines WHICH classes to use for each interface, not configuration VALUES",
  "environment": "development",
  "services": {
    "ILogger": {
      "module": "@network-monitor/mock-services",
      "className": "MockLogger",
      "isMock": true,
      "description": "Mock logger for development"
    },
    "IEventBus": {
      "module": "@network-monitor/infrastructure",
      "className": "EventBus",
      "isMock": false,
      "description": "Real event bus for proper service communication"
    },
    "IDatabaseService": {
      "module": "@network-monitor/mock-database",
      "className": "MockPrisma",
      "isMock": true,
      "description": "Mock database for offline development"
    },
    "IMonitoringTargetRepository": {
      "module": "@network-monitor/mock-database",
      "className": "MockMonitoringTargetRepository",
      "isMock": true,
      "description": "Mock repository with test data"
    },
    "ISpeedTestResultRepository": {
      "module": "@network-monitor/mock-database",
      "className": "MockSpeedTestResultRepository",
      "isMock": true,
      "description": "Mock repository with test data"
    },
    "IAlertRuleRepository": {
      "module": "@network-monitor/mock-database",
      "className": "MockAlertRuleRepository",
      "isMock": true,
      "description": "Mock repository with test data"
    },
    "IIncidentEventRepository": {
      "module": "@network-monitor/mock-database",
      "className": "MockIncidentEventRepository",
      "isMock": true,
      "description": "Mock repository with test data"
    },
    "INotificationRepository": {
      "module": "@network-monitor/mock-database",
      "className": "MockNotificationRepository",
      "isMock": true,
      "description": "Mock repository with test data"
    },
    "IPushSubscriptionRepository": {
      "module": "@network-monitor/mock-database",
      "className": "MockPushSubscriptionRepository",
      "isMock": true,
      "description": "Mock repository with test data"
    },
    "ITargetRepository": {
      "module": "@network-monitor/mock-database",
      "className": "MockMonitoringTargetRepository",
      "isMock": true,
      "description": "Mock legacy repository - alias for IMonitoringTargetRepository"
    },
    "ISpeedTestRepository": {
      "module": "@network-monitor/mock-database",
      "className": "MockSpeedTestResultRepository",
      "isMock": true,
      "description": "Mock legacy repository"
    },
    "IUserRepository": {
      "module": "@network-monitor/mock-database",
      "className": "MockUserRepository",
      "isMock": true,
      "description": "Mock user repository"
    },
    "IUserSpeedTestPreferenceRepository": {
      "module": "@network-monitor/repositories",
      "className": "UserSpeedTestPreferenceRepository",
      "isMock": false,
      "description": "Real user speed test preference repository"
    },
    "ISpeedTestUrlRepository": {
      "module": "@network-monitor/mock-repositories",
      "className": "MockSpeedTestUrlRepository",
      "isMock": true,
      "description": "Mock speed test URL repository with test data"
    },
    "ISpeedTestService": {
      "module": "@network-monitor/services",
      "className": "SpeedTestService",
      "isMock": false,
      "description": "Real speed test service for network performance testing"
    },
    "IMonitorService": {
      "module": "@network-monitor/mock-services",
      "className": "MockMonitorService",
      "isMock": true,
      "description": "Mock monitor service with hardcoded in-memory data"
    },
    "IAlertingService": {
      "module": "@network-monitor/services",
      "className": "AlertingService",
      "isMock": false,
      "description": "Real alerting service"
    },
    "INotificationService": {
      "module": "@network-monitor/services",
      "className": "NotificationService",
