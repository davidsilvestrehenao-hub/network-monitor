{
  "name": "network-monitor",
  "version": "0.0.0",
  "private": true,
  "packageManager": "bun@1.2.22",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "turbo run dev",
    "dev:web": "turbo run dev --filter=web",
    "dev:api": "turbo run dev --filter=api",
    "build": "turbo run build",
    "start": "turbo run start",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "db:generate": "turbo run db:generate --filter=@network-monitor/database",
    "db:push": "turbo run db:push --filter=@network-monitor/database",
    "db:migrate": "turbo run db:migrate --filter=@network-monitor/database",
    "db:seed": "turbo run db:seed --filter=@network-monitor/database"
  },
  "type": "module",
  "devDependencies": {
    "@types/node": "^20.11.26",
    "@typescript-eslint/eslint-plugin": "^7.6.0",
    "@typescript-eslint/parser": "^7.6.0",
    "eslint": "^9.13.0",
    "eslint-plugin-solid": "^0.14.3",
    "turbo": "^2.5.8",
    "typescript": "^5.6.2",
    "vite": "^6.2.2"
  },
  "dependencies": {
    "@prisma/client": "^6.16.2",
    "@solidjs/meta": "^0.29.4",
    "@solidjs/router": "0.15.1",
    "@solidjs/start": "1.1.3",
    "@tailwindcss/postcss": "^4.1.13",
    "autoprefixer": "^10.4.21",
    "chart.js": "^4.5.0",
    "chartjs-adapter-date-fns": "^3.0.0",
    "date-fns": "^4.1.0",
    "postcss": "^8.5.6",
    "prisma": "^6.16.2",
    "solid-chartjs": "^1.3.11",
    "solid-js": "^1.9.9",
    "tailwindcss": "^4.1.13",
    "vinxi": "0.5.3",
    "winston": "^3.17.0",
    "zod": "^3.23.8"
  },
  "engines": {
    "node": ">=24"
  }
}
